{
  "name": "quotify-2-standalone",
  "productName": "Quotify 2 - Experimental",
  "version": "2.0.0",
  "description": "Advanced quote extraction with enhanced AI speaker detection - Experimental Version",
  "main": "server.js",
  "homepage": "https://quotify-v2.app",
  "author": {
    "name": "Quotify V2 Team",
    "email": "hello@quotify-v2.app"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:web\"",
    "dev:vite": "vite --port 5173",
    "dev:web": "wait-on http://localhost:5173 && node server.js",
    "dev:electron": "wait-on http://localhost:5174 && electron . --dev",
    "build": "vite build",
    "build:electron": "vite build && electron-builder",
    "start": "node server.js",
    "preview": "vite preview",
    "pwa": "node pwa-server.js",
    "start:pwa": "./start-pwa.sh",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:e2e": "playwright test",
    "docker:build": "docker build -t quotify-web .",
    "docker:run": "docker run -p 5174:5174 quotify-web"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-toast": "^1.2.14",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "form-data": "^4.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.24",
    "lucide-react": "^0.358.0",
    "openai": "^4.28.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-player": "^2.15.1",
    "react-virtualized": "^9.22.6",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vitest": "^1.3.1",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
